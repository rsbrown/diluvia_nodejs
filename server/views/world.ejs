<script type="text/javascript" charset="utf-8">
    document.body.style = "height: 100%; width: 100%;";
    document.style      = "height: 100%; width: 100%";
    
    $(document).ready(function() {
        new DiluviaController("<%=SOCKET_IO_HOSTNAME%>", <%=SOCKET_IO_OPTIONS%>);
    });
</script>

<div id="viewport_container" style="width: 100%; height: 100%; position: absolute; bottom: 0">
    <canvas id="viewport"></canvas>
</div>

<script type="text/javascript" charset="utf-8">
    var vpc = document.getElementById("viewport_container");
    vpc.parentNode.removeChild(vpc);
    document.body.appendChild(vpc);
</script>